<div class="titlepage">
<h3>No matches found. Send an invitation to register:</h4><br>

<%= form_for resource, :as => resource_name, :url => invitation_path(resource_name), :html => {:method => :post} do |f| %>
  <%= devise_error_messages! %>

<% resource.class.invite_key_fields.each do |field| -%>
  <p><%= f.label field %>
  <%= f.text_field field %>
<% end -%>

  <%= f.submit t("devise.invitations.new.submit_button") %></p>
<% end %>
</div>
